{
  "name": "sfra",
  "version": "3.1.0",
  "description": "Storefront Reference Architecture",
  "main": "index.js",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "selenium-start": "selenium-standalone start",
    "copy": "node custom-scripts/makeFile/build.js",
    "test": "sgmf-scripts --test test/unit/**/*.js",
    "test:appium": "node bin/Makefile functional -- appium -- ",
    "cover": "sgmf-scripts --cover",
    "test:integration": "node bin/Makefile integration -- ",
    "test:functional": "node bin/Makefile functional -- ",
    "test:functional sanity": "node bin/Makefile functional -- sanity",
    "test:functional:docker": "bin/test-functional-docker.sh",
    "test:functional:sauce": "./node_modules/.bin/wdio test/functional/webdriver/wdio.conf.js ",
    "compile:scss": "sgmf-scripts --compile css",
    "compile:js": "sgmf-scripts --compile js",
    "compile:fonts": "node bin/Makefile compileFonts",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "sgmf-scripts --lint css",
    "lint:js": "sgmf-scripts --lint js",
    "upload": "sgmf-scripts --upload",
    "uploadCartridge": "sgmf-scripts --uploadCartridge app_storefront_base && sgmf-scripts --uploadCartridge modules",
    "watch": "sgmf-scripts --watch",
    "release": "node bin/Makefile release --"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/demandware/storefront-base.git"
  },
  "author": "Ilya Volodin <ivolodin@demandware.com>",
  "license": "ISC",
  "homepage": "https://bitbucket.org/demandware/storefront-base#readme",
  "devDependencies": {
    "@tridnguyen/config": "^2.3.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "chalk": "^1.1.3",
    "cheerio": "0.22.0",
    "cldr-data": "^31.0.2",
    "dwupload": "^3.4.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-sitegenesis": "~1.0.0",
    "globalize": "^1.1.1",
    "istanbul": "^0.4.4",
    "lodash": "^4.14.2",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.15.0",
    "moment-timezone": "^0.5.14",
    "properties-parser": "^0.3.0",
    "proxyquire": "1.7.4",
    "request-promise": "^4.2.2",
    "selenium-standalone": "^6.15.3",
    "sgmf-scripts": "~1.1.2",
    "shelljs": "^0.7.0",
    "sinon": "^1.17.4",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.1.0",
    "url": "^0.11.0",
    "wdio-allure-reporter": "^0.8.3",
    "wdio-dot-reporter": "0.0.10",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-sauce-service": "~0.1",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.13.2",
    "xml2js": "^0.4.17"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "4.4.1",
    "cleave.js": "^1.0.2",
    "flag-icon-css": "^2.3.0",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.1",
    "mochawesome": "^3.1.1",
    "mochawesome-report-generator": "^3.1.5",
    "wdio-mochawesome-reporter": "^2.0.1"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
  "packageName": "app_storefront_base"
}
